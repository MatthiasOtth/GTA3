{
    "model": "gta3",
    "dataset": "zinc",
    "model_params": {
        "hidden_dim": 48,
        "out_dim": 48,
        "phi": "sigmoid_shift",
        "alpha": "per_layer",
        "alpha_init": 0,
        "alpha_weight": 0,
        "num_heads": 4,
        "num_layers": 4,
        "pos_encoding": "wl",
        "pos_enc_dim": 8,
        "dropout": 0.5,
        "residual": true,
        "batch_norm": false,
        "layer_norm": true,
        "attention_bias": true
    },
    "train_params": {
        "lr": 5e-4,
        "lr_alpha": 5e-2,
        "weight_decay": 0.0,
        "lr_threshold": 1e-5,
        "seed": 0,
        "batch_size": 128,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 5
    },
    "logging": {
        "save_dir": "logs/zinc",
        "name": "zinc_gta3_4layer_sigmoid_shift_test"
    }
}