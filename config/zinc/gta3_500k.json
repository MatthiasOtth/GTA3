{
    "model": "gta3",
    "dataset": "zinc",
    "model_params": {
        "hidden_dim": 60,
        "out_dim": 72,
        "phi": "alpha_pow_dist_exp",
        "alpha": "per_layer",
        "alpha_init": -1.0,
        "alpha_weight": 0,
        "num_heads": 4,
        "num_layers": 16,
        "pos_encoding": "none",
        "pos_enc_dim": 8,
        "residual": true,
        "batch_norm": false,
        "layer_norm": true,
        "attention_bias": true
    },
    "train_params": {
        "lr": 1e-3,
        "weight_decay": 0.0,
        "lr_threshold": 1e-5,
        "seed": 0,
        "batch_size": 128,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 5
    },
    "logging": {
        "save_dir": "logs/zinc",
        "name": "zinc_gta3_500k_init-1"
    }
}