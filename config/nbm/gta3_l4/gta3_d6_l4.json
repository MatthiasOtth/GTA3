{
    "model": "gta3",
    "dataset": "neighborsmatch",
    "model_params": {
        "hidden_dim": 64,
        "out_dim": 64,
        "phi": "identity",
        "alpha": "fixed",
        "alpha_init": 0.0,
        "alpha_init_kwargs": {
            "lb": -15.0,
            "ub": 15.0
