        "lr_alpha": 1e-2,
        "weight_decay": 0.0,
        "seed": 1,
        "batch_size": 256,
        "tree_depth": 6,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 100,
        "lr_threshold": 1e-5
    },
    "logging": {
        "save_dir": "logs/nbm",
        "name": "nbm_d6_l4_lg"
    }
