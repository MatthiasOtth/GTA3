{
    "model": "gta3",
    "dataset": "cluster",
    "model_params": {
        "hidden_dim": 56,
        "out_dim": 128,
        "phi": "identity",
        "alpha": "fixed",
        "alpha_init": 0.0,
        "alpha_init_kwargs": {
            "lb": -10.0,
            "ub": 10.0
        },
        "alpha_weight": 0,
        "num_heads": 8,
        "num_layers": 16,
        "pos_encoding": "none",
        "pos_enc_dim": 8,
        "dropout": 0.1,
        "residual": true,
        "batch_norm": false,
        "layer_norm": true,
        "attention_bias": true
    },
    "train_params": {
        "lr": 5e-4,
        "lr_alpha": 1e-1,
        "weight_decay": 0.0,
        "lr_threshold": 1e-5,
        "seed": 0,
        "batch_size": 64,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 5
    },
    "logging": {
        "save_dir": "logs/cluster",
        "name": "cluster_gta3_500k_lr=5e-4_localglobalheads"
    }
}