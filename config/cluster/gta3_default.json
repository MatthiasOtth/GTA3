{
    "model": "gta3",
    "model_params": {
        "hidden_dim": 64,
        "out_dim": 64,
        "phi": "inverse_hops",
        "alpha": "per_layer",
        "alpha_init": 1,
        "alpha_weight": 0,
        "num_heads": 4,
        "num_layers": 4,
        "residual": true,
        "batch_norm": false,
        "layer_norm": true,
        "attention_bias": true
    },
    "train_params": {
        "lr": 1e-4,
        "max_epochs": 5,
        "valid_interval": 2,
        "seed": 0,
        "batch_size": 64
    },
    "logging": {
        "save_dir": "logs/cluster",
        "name": "cluster_gta3"
    }
}